      eyeColor: '{{random("blue", "brown", "green")}}',
      name: {
        first: '{{firstName()}}',
        last: '{{surname()}}'
      },
      work_history: [
        {
          'repeat(2,5)': {
            company: '{{company().toUpperCase()}}'
          }
        }
      ],
      phone: '+1 {{phone()}}',
