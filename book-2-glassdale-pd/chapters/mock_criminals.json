          }
        }
      ],
      convictions: [
        {
        'repeat(2,6)': {
          crime: '{{random("arson","murder","grand theft","vandalism","assault","battery","conspiracy to commit murder","tax evasion","theft","assault with a deadly weapon","manslaughter","blackmail","bribery","burglary","forgery","fraud","mail fraud","larceny","kidnapping","false imprisonment","criminal negligence","child abuse","animal abuse")}}',
